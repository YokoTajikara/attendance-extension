{
	"manifest_version": 3,
	"name": "出勤管理アプリ",
	"version": "1.0.0",
	"description": "Google認証で出勤管理を行うChrome拡張",
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "icons/icon16.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		}
	},
	"permissions": [
		"identity",
		"storage"
	],
	"host_permissions": [
		"https://*.supabase.co/*"
	],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}